    "cors": "^2.8.5",
    "dotenv": "^6.1.0",
    "eslint": "^5.9.0",
    "express": "^4.16.4",
    "gravatar": "^1.7.0",
    "jsonwebtoken": "^8.4.0",
    "lodash": "^4.17.11",
    "mongoose": "^5.3.13",
    "nodemon": "^1.18.6",
    "validator": "^10.9.0"
  }
